{
  "name": "weya/blade",
  "type": "project",
  "description": "weya blade",
  "keywords": ["blade"],
  "license": "MIT",
  "require": {
    "php": "^7.2|^8.0",
    "ext-json": "*",
    "filp/whoops": "^2.9",
    "illuminate/view": "^8.0",
    "symfony/console": "^4.4",
    "symfony/http-foundation": "^4.4",
    "symfony/var-dumper": "^5.2"
  },
  "require-dev": {},
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helper.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
